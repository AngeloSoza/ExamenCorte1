Proceso convertirKbaGb
	Definir Kb Como Real;
	Definir Gb Como Real;
	Definir Conver Como Real;
	Gb <- 1024;
	Escribir "Sea Bienvenido al sistema de conversion de Kb a GB";
	Escribir "El cambio de KB a GB es de 1024';
	Escribir "Ingresar el dato (en Kb) que se quiere pasar a GB";
	Leer Kb;
	Conver <- (Kb/Gb/Gb);
	Escribir "El resultado de la ecuación es de :", Conver;
	Escribir "El numero en Kb ingresado fue de:", Kb;
FinProceso
